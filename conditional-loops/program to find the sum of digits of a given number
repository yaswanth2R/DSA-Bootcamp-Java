import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    System.out.print("Enter a Number : ");
	    int num = sc.nextInt();
	    int rem,sum=0;
	    int i;
	    while(num!=0)
	    {
	        rem = num%10;
	        sum = sum+rem;
	        num = num/10;
	    }
	    System.out.print("The Sum of Digits are : "+sum);
	}
}
