import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc = new Scanner(System.in);
	    System.out.print("Enter the Number : ");
	    int num = sc.nextInt();
	    int i;
	    int mid=num/2;
	    System.out.println("Factors are : ");
	    for(i=1;i<=mid;i++)
	    {
	         if(num% i == 0)
	        {
	            System.out.println("" +i);
	        }
	    }
	    System.out.println(""+num);
	}
}
