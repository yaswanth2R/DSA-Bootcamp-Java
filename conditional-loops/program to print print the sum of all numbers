import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc = new Scanner(System.in);
	    System.out.print("Enter the Numbers : ");
	    int num ,sum = 0;
	    while(true)
	    {
	        num = sc.nextInt();
	        if(num==0)
	            break;
	        sum = sum+num;
	    }
	    System.out.print("Sum Of the Numbers is : "+sum);
	}
}
